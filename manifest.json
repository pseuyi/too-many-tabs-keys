{
  "name": "too many tabs keys",
  "description": "keyboard for people who have too many tabs open",
  "version": "1.0",
  "incognito": "split",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
 "background": {
  "scripts": ["Tone.js", "main.js"]
 },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
