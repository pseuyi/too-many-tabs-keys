{
  "name": "too many tabs keys",
  "description": "keyboard for people who have too many tabs open",
  "version": "1.0",
  "incognito": "split",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "default_popup": "index.html",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "manifest_version": 2,
}
